{
  "name": "@piranna/rpc",
  "version": "0.2.1",
  "description": "Transport agnostic and protocol polyglot RPC",
  "type": "module",
  "main": "index.cjs",
  "module": "lib",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./index.cjs"
    },
    "./JsonRpc": {
      "import": "./lib/JsonRpc.js",
      "require": "./JsonRpc.cjs"
    },
    "./XmlRpc": {
      "node": {
        "import": "./lib/XmlRpc_node.js",
        "require": "./XmlRpc_node.cjs"
      },
      "import": "./lib/XmlRpc.js",
      "require": "./XmlRpc.cjs"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "prepare": "npm run build",
    "test": "node --experimental-vm-modules node_modules/.bin/jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/piranna/rpc.git"
  },
  "keywords": [
    "transfast",
    "client",
    "websocket",
    "datachannel",
    "webrtc",
    "p2p",
    "json",
    "jsonrpc",
    "xmlrpc"
  ],
  "author": "Jesús Leganés-Combarro 'piranna' <piranna@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/piranna/rpc/issues"
  },
  "homepage": "https://github.com/piranna/rpc#readme",
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "lib/**/*.js"
    ],
    "resolver": "jest-node-exports-resolver"
  },
  "devDependencies": {
    "jest-cli": "^27.3.1",
    "jest-node-exports-resolver": "github:Mafalda-SFU/jest-node-exports-resolver",
    "rollup": "^2.58.0"
  },
  "dependencies": {
    "@xmldom/xmldom": "^0.7.5",
    "error-to-json": "^2.0.0",
    "object-path": "^0.11.8",
    "xmlrpc-serialization": "piranna/XmlRPC-serialization"
  }
}
