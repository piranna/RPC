{
  "name": "@piranna/rpc",
  "version": "0.2.4",
  "description": "Transport agnostic and protocol polyglot RPC",
  "type": "module",
  "module": "lib",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./index.cjs"
    },
    "./JsonRpc": {
      "import": "./lib/JsonRpc.js",
      "require": "./JsonRpc.cjs"
    },
    "./XmlRpc": {
      "node": {
        "import": "./lib/XmlRpc_node.js",
        "require": "./XmlRpc_node.cjs"
      },
      "import": "./lib/XmlRpc.js",
      "require": "./XmlRpc.cjs"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "prepare": "npm run build",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/piranna/rpc.git"
  },
  "keywords": [
    "transfast",
    "client",
    "websocket",
    "datachannel",
    "webrtc",
    "p2p",
    "json",
    "jsonrpc",
    "xmlrpc"
  ],
  "author": "Jesús Leganés-Combarro 'piranna' <piranna@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/piranna/rpc/issues"
  },
  "homepage": "https://github.com/piranna/rpc#readme",
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "lib/**/*.js"
    ],
    "projects": [
      {
        "displayName": "jsdom",
        "resolver": "jest-node-exports-resolver",
        "testEnvironment": "jsdom"
      },
      {
        "displayName": "node",
        "resolver": "jest-node-exports-resolver",
        "testEnvironment": "node"
      }
    ]
  },
  "devDependencies": {
    "jest-cli": "^27.5.0",
    "jest-node-exports-resolver": "^1.1.5",
    "rollup": "^2.67.1"
  },
  "dependencies": {
    "@xmldom/xmldom": "^0.8.0",
    "error-to-json": "^2.0.0",
    "object-path": "^0.11.8",
    "xmlrpc-serialization": "^3.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
